mProjects/login-user/src/pages/SignIn.js",[],"/Users/sharonrosa/WebstormProjects/login-user/src/pages/About.js",[],"/Users/sharonrosa/WebstormProjects/login-user/src/pages/SignUp.js",[],"/Users/sharonrosa/WebstormProjects/login-user/src/pages/Home.js",[],"/Users/sharonrosa/WebstormProjects/login-user/src/context/AuthContext.js",["58"],"import React, {createContext, useState, useEffect, useContext} from 'react';\n\nconst AuthContext = createContext({});\n\nfunction AuthContextProvider({children}) {\n    const [authState, setAuthState] = useState({\n        status: 'pending',\n        error: null,\n        user: null,\n    })\n\n    useEffect(() => {\n        // haal uit de local storage de JWT token\n        // als die er niet is, kkunnen we gewoon verder\n        // als die token er wel is, dan betekend dat dat de applicatie herstart\n        // en dan willen de nog even onze gebruikersdata (username, etc..)  ophalen.\n\n\n        setTimeout(() => {\n            // er is geen token, dus we beginnen met een schone lei\n            setAuthState({\n                ...authState,\n                status: 'done',\n            })\n        },2000)\n    }, []);\n\n    function login(data) {\n        localStorage.setItem('token', data.accessToken);\n\n        setAuthState({\n            ...authState,\n            user: {\n                username: data.username,\n                email: data.email,\n                roles: data.roles,\n            }\n        })\n\n    }\n\n    function logout() {\n        localStorage.clear();\n        setAuthState({\n            ...authState,\n            user: null,\n        })\n    }\n\n    return (\n        <AuthContext.Provider value={{...authState, login, logout}}>\n            {authState.status === 'done' && children}\n            {authState.status === 'pending' && <p>Loading...</p>}\n        </AuthContext.Provider>\n    );\n}\n\nfunction useAuthState() {\n    const authState = useContext(AuthContext);\n    // iemand is geauthoriseerd wanneer de status === 'done'\n    // en als er een gebruiker in de authState staat\n    const isDone = authState.status === 'done';\n    const isAuthenticated = authState.user !== null && isDone;\n\n    //console.log('Ik ben authenticated:', isAuthenticated);\n\n    return {\n        ...authState,\n        isAuthenticated: isAuthenticated,\n    }\n}\n\nexport {\n    AuthContext,\n    useAuthState,\n    AuthContextProvider,\n}\n","/Users/sharonrosa/WebstormProjects/login-user/src/components/SignUpForm/IndexSignUpForm.js",[],"/Users/sharonrosa/WebstormProjects/login-user/src/components/SignInForm/IndexSignInForm.js",["59"],"/Users/sharonrosa/WebstormProjects/login-user/src/components/Header.js",["60"],"/Users/sharonrosa/WebstormProjects/login-user/src/routing/PrivateRoute.js",[],{"ruleId":"61","replacedBy":"62"},{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","severity":1,"message":"66","line":26,"column":8,"nodeType":"67","endLine":26,"endColumn":10,"suggestions":"68"},{"ruleId":"65","severity":1,"message":"69","line":28,"column":8,"nodeType":"67","endLine":28,"endColumn":25,"suggestions":"70"},{"ruleId":"65","severity":1,"message":"69","line":15,"column":8,"nodeType":"67","endLine":15,"endColumn":25,"suggestions":"71"},"no-native-reassign",["72"],"no-negated-in-lhs",["73"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'authState'. Either include it or remove the dependency array. You can also do a functional update 'setAuthState(a => ...)' if you only need 'authState' in the 'setAuthState' call.","ArrayExpression",["74"],"React Hook useEffect has a missing dependency: 'history'. Either include it or remove the dependency array.",["75"],["76"],"no-global-assign","no-unsafe-negation",{"desc":"77","fix":"78"},{"desc":"79","fix":"80"},{"desc":"79","fix":"81"},"Update the dependencies array to be: [authState]",{"range":"82","text":"83"},"Update the dependencies array to be: [history, isAuthenticated]",{"range":"84","text":"85"},{"range":"86